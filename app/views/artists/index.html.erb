<div class="container">
  <div class="row">
      <% @artists.each do |artist| %>
        <div class="artist">
           <img src="<%= artist.picture %>" alt="">
              <p class="name"><%= artist.name %></p>
              <p><%= link_to 'See More', artist_path(artist) %></p>
        </div>
      <% end %>
  </div>
</div>
